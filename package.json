{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "analyze": "ANALYZE=true npm run build",
    "clean": "rm -rf .next out dist",
    "db:generate-types": "supabase gen types typescript --local > types/supabase.ts",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase migration up",
    "db:seed": "supabase seed",
    "deploy:production": "./scripts/deploy-to-production.sh",
    "add:analytics": "./scripts/add-analytics-to-existing-db.sh",
    "verify:production": "node scripts/verify-production.js",
    "dev:setup": "./scripts/dev-setup.sh",
    "dev:reset": "psql crownsync_dev -c 'DROP SCHEMA public CASCADE; CREATE SCHEMA public;' && npm run dev:setup",
    "dev:seed": "npx tsx scripts/generate-marco-bicego-data.ts",
    "test:db": "psql crownsync_dev -f __tests__/database/campaign_analytics.sql",
    "test:service": "npx tsx scripts/test-service-layer.ts",
    "test:api": "npx tsx scripts/test-api-layer.ts",
    "test:dynamic": "npx tsx scripts/test-dynamic-data.ts",
    "test:phase5": "chmod +x scripts/run-phase5-tests.sh && ./scripts/run-phase5-tests.sh",
    "test:all": "npm run test:phase5",
    "prepare": "husky install || true",
    "pre-commit": "lint-staged",
    "check-all": "npm run type-check && npm run lint && npm run format:check && npm run test:ci"
  },
